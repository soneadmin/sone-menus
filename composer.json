{
    "name": "sequelone/sone-menus",
    "description": "Menus for sOne Admin.",
    "keywords": ["laravel", "php", "admin", "sone", "menus"],
    "license": "Apache-2.0",
    "homepage": "https://github.com/soneadmin/sone-menus",
    "authors": [
        {
            "name": "Andrej Kopp",
            "homepage": "https://github.com/soneadmin",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Sequelone\\Sone\\Menus\\": "src/",
            "Sequelone\\Sone\\Menus\\Database\\Seeds\\": "database/seeds/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sequelone\\Sone\\Menus\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.1",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^5.0|^6.0",
        "orchestra/database": "^5.0|^6.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Sequelone\\Sone\\Menus\\SoneMenusServiceProvider"
            ]
        }
    }
}
